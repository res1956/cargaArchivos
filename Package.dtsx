ce(Referencia3,'''''''','''''''''''')&#xA;&#x9;  , clase , Marca , codigo , Homologocodigo, replace(Referencia1,'''''''','''''''''''')&#xA;&#x9;   from eac_fasecolda_wk ')&#xA;&#xA;----------------------------------------------------------------&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
